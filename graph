// написать программу, определяющую, является ли неориентированный граф, заданный матрицей смежности n*n, деревом.
// Для определения графа была написана программа, которая считает кол-во рёбер графа и проверяет, является ли он связным. Граф является деревом, 
// если граф связный и в графе n-1 ребро. Если эти два условия выполняются, то в графе нет циклов. 
// 1
#include <iostream>

using namespace std;

int w[100][100], v[100], n;
void pois(int j) {
	v[j] = 1;
	for (int i = 0; i < n; i++) {
		if (w[i][j] && !v[i])
			poisk(i);
	}
}

int main() {
	cin >> n;
	int k = 0, a = 1;
	for (int i = 0; i < n; i++) {
		for (int j = 0; j < n; j++) {
			cin >> w[i][j];
			if (w[i][j]) 
				k++;
		}
	}
	poisk(0);
	for (int i = 0; i < n; i++) {
		if (!v[i]) 
			a = 0;
	}
	cout << ((a && k / 2 == n - 1) ? "Derevo" : "Ne derevo");
	return 0;
}
